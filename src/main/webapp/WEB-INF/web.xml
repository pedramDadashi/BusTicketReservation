<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
    <servlet-name>signupservlet</servlet-name>
    <servlet-class>org.example.controller.SignUpServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>loginservelt</servlet-name>
    <servlet-class>org.example.controller.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>searchservlet</servlet-name>
    <servlet-class>org.example.controller.SearchServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>chooseticketservlet</servlet-name>
    <servlet-class>org.example.controller.ChooseTicketServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>passengerinfoservlet</servlet-name>
    <servlet-class>org.example.controller.PassengerInfoServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>bookinghistoryservlet</servlet-name>
    <servlet-class>org.example.controller.BookingHistoryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>signupservlet</servlet-name>
    <url-pattern>/signupservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>loginservelt</servlet-name>
    <url-pattern>/loginservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>searchservlet</servlet-name>
    <url-pattern>/searchservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>chooseticketservlet</servlet-name>
    <url-pattern>/chooseticketservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>passengerinfoservlet</servlet-name>
    <url-pattern>/passengerinfoservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>bookinghistoryservlet</servlet-name>
    <url-pattern>/bookinghistoryservlet</url-pattern>
  </servlet-mapping>
</web-app>
